{
  "name": "slingr",
  "version": "1.0.0",
  "description": "Test framework for automat using webdriverio & typescript.",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf typescript/ && rimraf allure-results/",
    "clean-build": "npm run clean && npm run build",
    "appium": "appium",
    "appium-chrome": "appium --allow-insecure chromedriver_autodownload",
    "web": "wdio ./config/wdio.web.config.js --cucumberOpts.tagExpression \"@run\"",
    "report": "allure generate ./allure-results",
    "start": "npm run clean-build && npm run web",
    "clean-report": "allure generate --clean ./allure-results",
    "open-report": "allure open"
  },
  "keywords": [
    "appium",
    "webdriverio",
    "typescript",
    "mobile automation",
    "behaviour driven development",
    "bdd",
    "selenium",
    "webdriverJS",
    "gherkin",
    "automation testing"
  ],
  "author": "Axel Poretti",
  "license": "MIT",
  "dependencies": {
    "@types/chai": "^4.2.17",
    "@types/cucumber": "^4.0.7",
    "@types/faker": "^4.1.12",
    "@types/node": "^10.17.31",
    "@types/webdriverio": "^4.10.2",
    "@wdio/allure-reporter": "^6.11.0",
    "@wdio/appium-service": "^6.12.1",
    "@wdio/cli": "7.5.2",
    "@wdio/cucumber-framework": "7.5.2",
    "@wdio/local-runner": "7.5.2",
    "@wdio/spec-reporter": "7.5.2",
    "@wdio/sync": "7.5.2",
    "allure-commandline": "^2.13.8",
    "appium": "^1.20.2",
    "appium-chromedriver": "^4.27.1",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "chai": "4.3.4",
    "chromedriver": "90.0.0",
    "cucumber": "^6.0.5",
    "express": "^4.17.1",
    "faker": "5.5.3",
    "localtunnel": "^2.0.1",
    "ngrok": "4.0.1",
    "rimraf": "3.0.2",
    "ts-node": "9.1.1",
    "tslint": "6.2.0",
    "twilio": "^3.61.0",
    "typescript": "4.2.4",
    "wdio-chromedriver-service": "7.0.0"
  }
}
